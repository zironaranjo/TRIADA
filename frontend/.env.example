##############################
# TRIADA Frontend - .env.example
##############################

## ───── Backend API ─────
# URL del backend NestJS
# Desarrollo local:
VITE_API_URL=http://localhost:3000

# Producción (ejemplo):
# VITE_API_URL=https://api.triadak.io

## ───── Supabase ─────
# URL del proyecto Supabase
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Public anon key de Supabase
VITE_SUPABASE_ANON_KEY=your-anon-key-here

## ───── Auth bypass (solo staging) ─────
# Si se establece a "true", ProtectedRoute no requerirá login.
# Úsalo únicamente en entornos de pruebas, nunca en producción.
VITE_BYPASS_AUTH=false

